@{ // Read input
    var EmailAddress = Request["EmailAddress"];
    var Message = Request["Message"];
    try
    {
        // Send email
        WebMail.Send(to: "customerservice@smfreelancing.com", subject: "Email From -" + EmailAddress, body: Message);
    }
    catch (Exception ex)
    {
        <text>@ex</text>
    }
} 
